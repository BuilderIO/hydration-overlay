{
  "name": "@builder.io/react-hydration-overlay",
  "description": "React utility for descriptive hydration mismatch errors.",
  "version": "0.2.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "hydration",
    "overlay",
    "builder.io"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BuilderIO/hydration-overlay.git"
  },
  "main": "./dist/HydrationOverlay.mjs",
  "module": "./dist/HydrationOverlay.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/HydrationOverlay.d.ts",
        "default": "./dist/HydrationOverlay.mjs"
      },
      "require": {
        "types": "./dist/HydrationOverlay.d.cts",
        "default": "./dist/HydrationOverlay.cjs"
      }
    },
    "./next": {
      "import": {
        "types": "./dist/next.d.ts",
        "default": "./dist/next.mjs"
      },
      "require": {
        "types": "./dist/next.d.cts",
        "default": "./dist/next.cjs"
      }
    },
    "./webpack": {
      "import": {
        "types": "./dist/webpack.d.ts",
        "default": "./dist/webpack.mjs"
      },
      "require": {
        "types": "./dist/webpack.d.cts",
        "default": "./dist/webpack.cjs"
      }
    },
    "./hydration-overlay-initializer": "./dist/hydration-overlay-initializer.js"
  },
  "scripts": {
    "build": "bunchee",
    "pkg:attw": "attw --pack .",
    "pkg:publint": "publint --strict",
    "lint-pkg": "pnpm pkg:publint && pnpm pkg:attw"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "webpack": ">=4",
    "@spotlightjs/spotlight": "^1.2.11"
  },
  "peerDependenciesMeta": {
    "@spotlightjs/spotlight": {
      "optional": true
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@types/beautify": "^0.0.3",
    "@types/diff": "^5.0.9",
    "@types/node": "^20",
    "@types/pretty": "^2.0.3",
    "@types/react": "^18.2.43",
    "bunchee": "^5.5.0",
    "next": "14.0.4",
    "publint": "^0.2.11",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "beautify": "^0.0.8",
    "react-diff-viewer-continued": "^4.0.3"
  }
}
